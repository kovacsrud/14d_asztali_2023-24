<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Maui_Navigacio.Pages.Page2"
             Title="Page2">
    <VerticalStackLayout>
        <Label 
            Text="Page2"
            FontSize="Header"
            FontAttributes="Bold"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Label x:Name="labelTxt" FontSize="Body" />
        <Label Text="{Binding Szoveg}"/>
        <Button Text="StartPage" Clicked="Button_Clicked"/>
        <Button Text="EndPage" Clicked="Button_Clicked_1"/>
    </VerticalStackLayout>
</ContentPage>